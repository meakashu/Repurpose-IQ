{
  "name": "repurposeiq",
  "version": "2.0.0",
  "description": "RepurposeIQ - Intelligent Drug Repurposing Platform",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm install && npm run build",
    "vercel-build": "npm install && cd client && npm install && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "pharmaceutical",
    "ai",
    "multi-agent",
    "crewai",
    "groq",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "all": "^0.0.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.5.0",
    "chromadb": "^1.8.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "groq-sdk": "^0.3.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
